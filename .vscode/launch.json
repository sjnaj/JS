{
    "configurations": [
        {
            "name": "Launch filterRange.js",
            "program": "${workspaceFolder}/dataType/filterRange.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch domTest2.html",
            "file": "${workspaceFolder}/domTest2.html",
            "pathMappings": [
                {
                    "url": "http://localhost",
                    "path": "${workspaceFolder}"
                }
            ],
            "firefoxArgs": [
                "-start-debugger-server",
                "-no-remote"
            ],
            "log": {
                "fileName": "${workspaceFolder}/log.txt",
                "fileLevel": {
                    "default": "Debug",
                },
                "consoleLevel": {
                    "PathConversion": "Debug",
                    "default": "Error"
                }
            }
        },
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch basic2.html",
            "file": "${workspaceFolder}/basic2.html"
        },
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch test.html",
            "file": "${workspaceFolder}/test.html"
        },
        {
            "name": "Launch test.js",
            "program": "${workspaceFolder}/test.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }
    ]
}